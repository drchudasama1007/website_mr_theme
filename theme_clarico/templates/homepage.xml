    <template id="news_snippet" name="LETEST NEWS">
        <section>
            <t t-set="blogs"
               t-value="website.get_letest_news() if website else None"/>
            <t t-if="blogs">
                <div class="row">
                    <div class="col-md-2 col-sm-2 col-1">
                    </div>
                    <div class="col-md-8 col-sm-8 col-10">
                        <div class="title_style">
                            <h2 class="font-weight-bold text-black pt16 mb0 pl-0" style="font-size: 17px;">
                                LETEST NEWS
                            </h2>
                            <div class="border_right"></div>
                            <div class="owl-carousel news_prev staff-list">
                                <t t-foreach="blogs['blogs']" t-as="blog">
                                    <div class="single-staff-item">
                                        <img class="img-fluid"
                                             src="/theme_clarico/static/src/img/footer/footer_1.jpg"/>
                                        <h4 class="text-black pt24 text-truncate">
                                            <t t-esc="blog.name"/>
                                        </h4>

                                        <p>
                                            <span class="pb8"
                                                  style="font-size:12px;text-transform: uppercase;padding-right: 10px;">
                                                <i class="fa fa-minus pr-1" style="color:#fdd642;"/>
                                                <t t-esc="blog.tags_name"/>
<!--                                                <t t-foreach="blog.tag_ids" t-as="tags">-->
<!--                                                    <span class="pr-2">-->
<!--                                                        <t t-esc="tags.name"/>-->
<!--                                                    </span>-->
<!--                                                </t>-->
                                                <i class="fa fa-pencil pl-2"
                                                   style="color:#fdd642;border-left: 1px solid gray;"/>
                                                <t t-esc="blog.author_id.name"/>
                                            </span>
                                        </p>

                                        <p style="font-size:12px">
                                            <t t-if="len(blog.subtitle) &gt; 80">
                                                <t t-esc="blog.subtitle[:80]"/>
                                                ...
                                            </t>
                                            <t t-else="">
                                                <t t-esc="blog.subtitle"/>
                                            </t>
                                        </p>
                                        <a t-attf-href="/blog/#{blog.blog_id.id}/post/#{blog.id}"
                                           class="btn pl-3 pr-3 pt8 mt24 pb8 mb40 text-black"
                                           style="background:#fdd642;font-size: 12px;">READ MORE
                                        </a>
                                    </div>
                                </t>
                            </div>
                        </div>
                    </div>
                </div>
            </t>
        </section>
    </template>

